{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "camelcase": "off",
        "no-useless-constructor": "off",
        "no-console": "off",
        "no-unused-vars": "off",
        "no-dupe-args": "off",
        "no-redeclare": "off",
        "@typescript-eslint/no-unused-vars": "off"
    }
}