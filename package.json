{
  "name": "MyStudium",
  "version": "1.0.0",
  "main": "server.ts",
  "license": "MIT",
  "scripts": {
    "build": "tsc --project tsconfig.build.json && tscpaths -p tsconfig.build.json -s ./src -o ./dist",
    "dev:server": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules src/shared/infra/http/server.ts",
    "ts:start": "ts-node  -r tsconfig-paths/register src/shared/infra/http/server.ts",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "test": "jest",
    "js:build": "tsc --project tsconfig.build.json && tscpaths -p tsconfig.build.json -s ./src -o ./dist",
    "start": "node dist/shared/infra/http/server.js",
    "js:warn": "node --trace-warnings dist/shared/infra/http/server.js"
  },
  "dependencies": {
    "apollo-server": "^2.18.2",
    "apollo-server-core": "^2.18.2",
    "aws-sdk": "^2.830.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.3.1",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "fast-glob": "^3.2.4",
    "graphql": "^15.3.0",
    "graphql-tools": "^7.0.2",
    "handlebars": "^4.7.6",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.2",
    "nodemailer": "^6.4.17",
    "nodemailer-mailgun-transport": "^2.0.2",
    "reflect-metadata": "^0.1.13",
    "remove-accents": "^0.4.2",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tscpaths": "^0.0.9",
    "tsyringe": "^4.3.0",
    "typeorm": "^0.2.28",
    "typescript": "^4.0.3",
    "uuid": "^8.3.1",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongodb": "^3.5.28",
    "@types/nodemailer": "^6.4.0",
    "@types/nodemailer-mailgun-transport": "^1.4.2",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.5.2",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "ts-node-dev": "^1.0.0-pre.63",
    "typescript-eslint": "^0.0.1-alpha.0"
  }
}
